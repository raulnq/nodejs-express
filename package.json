    "jwks-rsa": "^3.2.0",
    "knex": "^3.1.0",
    "morgan": "^1.10.1",
    "p-timeout": "^6.1.4",
    "pg": "^8.16.3",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
