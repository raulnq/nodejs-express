    "@eslint/js": "^9.32.0",
    "@faker-js/faker": "^10.1.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4"
  },
  "dependencies": {
    "@datalust/winston-seq": "^3.0.1",
